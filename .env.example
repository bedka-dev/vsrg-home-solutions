# Backend API Configuration
# URL of your Laravel backend API (no trailing slash)
VITE_BACKEND_API_URL=http://localhost:8000

# Google Maps Configuration
# Get your API key from: https://console.cloud.google.com/google/maps-apis
# Remember to set HTTP referrer restrictions in Google Cloud Console
VITE_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Application Configuration
# Environment: development, staging, production
VITE_APP_ENV=development

# Application Name
VITE_APP_NAME="vsrg-home-solutions"

# Business Contact Information
VITE_BUSINESS_PHONE="(972) 211-0909"
VITE_BUSINESS_EMAIL=info@vsrghomesolutions.com

# Feature Flags (optional - uncomment when needed)
# VITE_ENABLE_ANALYTICS=false
# VITE_ENABLE_CHAT=false

# Analytics (optional - add when ready)
# VITE_GA_MEASUREMENT_ID=G-XXXXXXXXXX
# VITE_GTM_ID=GTM-XXXXXXX

# Sentry Error Tracking (optional - add when ready)
# VITE_SENTRY_DSN=
# VITE_SENTRY_ENVIRONMENT=development
